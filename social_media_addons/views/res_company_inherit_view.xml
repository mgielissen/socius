<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="inherit_res_company_form" model="ir.ui.view">
        <field name="name">inherit.res.company.from</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="social_media.view_company_form_inherit_social_media"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='social_media']" position="inside">
                    <field name="social_skype" invisible="1"/>
                    <field name="social_whatsapp" invisible="1"/>
            </xpath>
        </field>
    </record>

</odoo>